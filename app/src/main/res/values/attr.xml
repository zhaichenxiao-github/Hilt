<?xml version="1.0" encoding="utf-8"?>
<resources>
<!--    自定义属性-->
    <declare-styleable name="EditTextView">
        <attr name="editTextName" format="string"/>
        <attr name="editTextColor" format="color"/>
        <attr name="editTextSize" format="dimension"/>
        <attr name="editTextNameVisibility" format="string">
            <enum name="gone" value="0x00000008"/>
            <enum name="visible" value="0x00000000" />
            <enum name="invisible" value="0x00000004" />
        </attr>
        <attr name="TextName" format="string"/>
        <attr name="TextColor" format="color"/>
        <attr name="TextSize" format="dimension"/>
        <attr name="TextNameVisibility" format="string">
            <enum name="gone" value="0x00000008"/>
            <enum name="visible" value="0x00000000" />
            <enum name="invisible" value="0x00000004" />
        </attr>
    </declare-styleable>

    <declare-styleable name="MyTitle">
        <attr name="textColor" format="color"/>
        <attr name="titleText" format="string"/>
        <attr name="leftText" format="string"/>
        <attr name="rightText" format="string"/>
    </declare-styleable>
</resources>